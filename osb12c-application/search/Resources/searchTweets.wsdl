<wsdl:definitions
     name="searchTweets"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/coherence/osb12c-application/search/searchTweets"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/coherence/osb12c-application/search/searchTweets"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:imp2="http://anteo-consulting.com/tweet"
     xmlns:imp1="http://xmlns.oracle.com/pcbpel/adapter/coherence/searchTweets/request"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    >
  <plt:partnerLinkType name="Query_plt" >
    <plt:role name="Query_role" >
      <plt:portType name="tns:Query_ptt" />
    </plt:role>
  </plt:partnerLinkType>
    <wsdl:types>
      <schema xmlns="http://www.w3.org/2001/XMLSchema" >
        <import namespace="http://anteo-consulting.com/tweet" schemaLocation="../../common/xsd/tweet.xsd" />
      </schema>
      <schema xmlns="http://www.w3.org/2001/XMLSchema" >
        <import namespace="http://xmlns.oracle.com/pcbpel/adapter/coherence/searchTweets/request" schemaLocation="searchTweetsRequest.xsd" />
      </schema>
    </wsdl:types>
    <wsdl:message name="Query_msg">
        <wsdl:part name="body" element="imp1:QueryRequest"/>
    </wsdl:message>
    <wsdl:message name="QueryResponse_msg">
        <wsdl:part name="body" element="imp2:tweets"/>
    </wsdl:message>
    <wsdl:portType name="Query_ptt">
        <wsdl:operation name="Query">
            <wsdl:input message="tns:Query_msg"/>
            <wsdl:output message="tns:QueryResponse_msg"/>
        </wsdl:operation>
    </wsdl:portType>
</wsdl:definitions>
