<wsdl:definitions name="RestService" 
targetNamespace="http://xmlns.oracle.com/chitchat/rest-server/RestService" 
xmlns:tns="http://xmlns.oracle.com/chitchat/rest-server/RestService" 
xmlns:inp1="http://anteo-consulting.com/tweet" 
xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
    <plnk:partnerLinkType name="RestService">
        <plnk:role name="RestServiceProvider" portType="tns:RestService_ptt"/>
    </plnk:partnerLinkType>
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://anteo-consulting.com/tweet" schemaLocation="tweet.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="POST_inputMessage">
        <wsdl:part name="request" element="inp1:tweet"/>
    </wsdl:message>
    <wsdl:message name="Rest_EmptyMessage"/>
    <wsdl:portType name="RestService_ptt">
        <wsdl:operation name="POST">
            <wsdl:input message="tns:POST_inputMessage"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="RestService_ptt-binding" type="tns:RestService_ptt">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="POST">
            <soap:operation soapAction="POST"/>
            <wsdl:input>
                <soap:body use="literal"/>
            </wsdl:input>
        </wsdl:operation>
    </wsdl:binding>
</wsdl:definitions>