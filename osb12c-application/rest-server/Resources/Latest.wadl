<?xml version = '1.0' encoding = 'UTF-8'?>
<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://xmlns.oracle.com/chitchat/rest-server/Latest" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="Latest">/chitchat/latest/[author]</doc>
   <grammars>
      <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://anteo-consulting.com/latest_request" schemaLocation="latest_request.xsd"/>
        </xsd:schema>
      <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://TargetNamespace.com/Latest_Get_response" schemaLocation="latest_response.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/chitchat/latest">
         <method name="GET" soa:wsdlOperation="latest">
            <request>
               <param name="author" style="query" soa:expression="$msg.request/tns:author" default="" type="xsd:string"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:latest_response" xmlns:cns="http://TargetNamespace.com/Latest_Get_response"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
