<?xml version = '1.0' encoding = 'UTF-8'?>
<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://xmlns.oracle.com/chitchat/rest-server/ChitChatRestService" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="ChitChatRestService">RestService for ChitChat</doc>
   <grammars>
      <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://anteoconsulting.com/ChitChatRestService_POST_request" schemaLocation="post_payload.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/chitchat">
         <method name="POST" soa:wsdlOperation="POST">
            <request>
               <representation mediaType="application/json" element="cns:post-root" xmlns:cns="http://anteoconsulting.com/ChitChatRestService_POST_request"/>
            </request>
            <response status="201"/>
         </method>
      </resource>
   </resources>
</application>
