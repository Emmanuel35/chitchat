<wsdl:definitions name="thread" targetNamespace="http://xmlns.oracle.com/osb12c-application/thread/thread" xmlns:tns="http://xmlns.oracle.com/osb12c-application/thread/thread" xmlns:inp1="http://anteo-consulting.com/tweet" xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
    <plnk:partnerLinkType name="thread">
        <plnk:role name="threadProvider" portType="tns:thread_ptt"/>
    </plnk:partnerLinkType>
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://anteo-consulting.com/tweet" schemaLocation="../../common/xsd/tweet.xsd"/>
        </xsd:schema>
        <schema targetNamespace="http://xmlns.oracle.com/osb12c-application/thread/thread" elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema">
            <element name="thread_params">
                <complexType>
                    <sequence>
                        <element name="thread" type="string"/>
                    </sequence>
                </complexType>
            </element>
        </schema>
    </wsdl:types>
    <wsdl:message name="thread_inputMessage">
        <wsdl:part name="request" element="tns:thread_params"/>
    </wsdl:message>
    <wsdl:message name="thread_outputMessage">
        <wsdl:part name="reply" element="inp1:tweets"/>
    </wsdl:message>
    <wsdl:portType name="thread_ptt">
        <wsdl:operation name="thread">
            <wsdl:input message="tns:thread_inputMessage"/>
            <wsdl:output message="tns:thread_outputMessage"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="thread_ptt-binding" type="tns:thread_ptt">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="thread">
            <soap:operation soapAction="thread"/>
            <wsdl:input>
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
</wsdl:definitions>