<?xml version= '1.0' encoding= 'UTF-8' ?>
<wsdl:definitions
     name="chitchat"
     targetNamespace="http://xmlns.oracle.com/chitchat/rest-server/chitchat"
     xmlns:tns="http://xmlns.oracle.com/chitchat/rest-server/chitchat"
     xmlns:inp1="http://anteoconsulting.com/chitchat_POST_request"
     xmlns:inp2="http://xmlns.oracle.com/chitchat/rest-server/chitchat"
     xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    >
    <plnk:partnerLinkType name="chitchat">
        <plnk:role name="chitchatProvider" portType="tns:chitchat_ptt"/>
    </plnk:partnerLinkType>
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://anteoconsulting.com/chitchat_POST_request" schemaLocation="../Schemas/tweet_schema.xsd"/>
        </xsd:schema>
        <schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xmlns.oracle.com/chitchat/rest-server/chitchat"
             elementFormDefault="qualified">
            <element name="empty">
                <complexType/>
            </element>
        </schema>
    </wsdl:types>
    <wsdl:message name="Rest_EmptyMessage">
        <wsdl:part name="Empty" element="tns:empty"/>
    </wsdl:message>
    <wsdl:portType name="chitchat_ptt">
        <wsdl:operation name="POST">
            <wsdl:input message="tns:POST_inputMessage"/>
            <wsdl:output message="tns:Rest_EmptyMessage"/>
        </wsdl:operation>
    </wsdl:portType>
</wsdl:definitions>
